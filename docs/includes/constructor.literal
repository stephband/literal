
<header class="@0-x1 @0-3x @1-x3 @1-4x @2-x3 @0-x-stretch">
    ${ data.filter((doc) => doc.type === 'constructor').map((doc) => `
    <h2 class="doctext-02" id="${ slugify(doc.name) }"><code>${ doc.name }()</code></h2>
    ${ doc.body }
    `) }
</header>

